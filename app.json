{
  "expo": {
    "name": "uContas",
    "slug": "accounts-plan-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "br.com.accountsplanapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.com.accountsplanapp",
      "infoPlist": {
        "CFBundleDisplayName": "uContas"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#622490",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "br.com.accountsplanapp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#622490",
          "dark": {
            "backgroundColor": "#622490"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "36de9b6e-b30d-4e84-947d-aedcb9e7ce56"
      }
    },
    "owner": "carlos.pedrassani"
  }
}
